{
    "name": "moririnson/laravel-line-notify",
    "description": "line notify for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shunsuke Mori",
            "email": "morimorim7180@gmail.com"
        }
    ],
    "require": {
    	"php": ">=7.0",
    	"guzzlehttp/guzzle": ">=6.0",
    	"illuminate/notifications": ">=6.0"
    },
    "require-dev": {
        "mockery/mockery": "*",
        "phpunit/phpunit": ">=6.0",
        "orchestra/testbench": ">=4.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
    	"psr-4": {
            "Moririnson\\LINENotify\\": "src"
    	}
    },
    "autoload-dev": {
    	"psr-4": {
            "Moririnson\\LINENotify\\Tests\\": "tests"
    	}
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/phpcs"
    }
}
